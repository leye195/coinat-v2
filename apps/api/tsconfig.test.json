{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "outDir": "./dist-test",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ES2019",
    "types": ["node"],
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": [
    "app/api/lib/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
