{
  "root": true,
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "jest": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier", "unused-imports"],
  "rules": {
    "unused-imports/no-unused-imports": "error"
  }
}
